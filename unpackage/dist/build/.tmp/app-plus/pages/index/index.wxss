/* 分段器头部 fixed 定位 */
.fixedit {
	width: 100%;
	position: fixed;
	z-index: 99;
	left: 0;
	top: 0;
}
.grace-news-list-imgs {
	max-height: 68px;
	overflow: hidden;
}
